


<div class="container">
  <div class="main_header">
    <div class="row">
      <div> <h2>10 Random Spelling Bee Words</h2></div>
    </div>
  </div>


<div class="container">
    <% @randomwords.each do |word| %>
      <div class="card">
        <div class="front">
          <%= image_tag word.pic %>
        </div>
        <div class="back">
          <h3><%= word.letters %></h3>
        </div>
      </div>
    <% end %>
</div>


</div>
